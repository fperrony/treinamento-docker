{
  "name": "ixcsfot/treinamento-docker-application",
  "description": "Treinamento Docker Application",
  "license": [
    "proprietary"
  ],
  "autoload": {
    "psr-4": {
      "IXCSoft\\TreinamentoDocker\\Application\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IXCSoft\\TreinamentoDocker\\Application\\Tests\\Integration\\": "tests/integration",
      "IXCSoft\\TreinamentoDocker\\Application\\Tests\\Unit\\": "tests/unit"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../domain"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "ext-xml": "*",
    "ext-memcached": "*",
    "ext-redis": "*",
    "ext-yaml": "*",
    "ext-zip": "*",
    "ixcsfot/treinamento-docker-domain": "dev-master",
    "pimple/pimple": "3.5.*",
    "slim/psr7": "1.6.*",
    "slim/slim": "4.11.*",
    "symfony/cache": "6.1.*",
    "symfony/yaml": "6.1.*"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^1.5.",
    "fzaninotto/faker": "^1.5.",
    "phpunit/phpunit": "^9.5",
    "mikey179/vfsstream": "^1.6"
  }
}
